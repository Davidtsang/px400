<p id="notice"><%= notice %></p>

<h1>Listing Icodes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>User</th>
      <th>Is used</th>
      <th>Used user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @icodes.each do |icode| %>
      <tr>
        <td><%= icode.code %></td>
        <td><%= icode.user_id %></td>
        <td><%= icode.is_used %></td>
        <td><%= icode.used_user_id %></td>
        <td><%= link_to 'Show', icode %></td>
        <td><%= link_to 'Edit', edit_icode_path(icode) %></td>
        <td><%= link_to 'Destroy', icode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Icode', new_icode_path %>
